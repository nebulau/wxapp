/* pages/attension/attension.wxss */
page{
  height: 100%;
}
.nofound {
  height: 100%;
  width: 100%;
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
  justify-content: center;
}
.itembox {
  background: #eeeeee;
  margin: 20rpx;
  justify-content: space-between;
  display: flex;
  flex-flow: row nowrap;
}
.focusitem {
  display: flex;
  width: 70%;
  flex-flow: column nowrap;
  background-color: grey;
}
.defocus {
  background-color: blanchedalmond;
  width: 30%;
  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
}