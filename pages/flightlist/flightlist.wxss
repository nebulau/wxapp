page {
  height: 100%;
}
.card {
    border: 2px solid #ffffff;
    border-radius: 5px;
    background-color: #ffffff;
    box-shadow: 4px 1px 1px #cccccc;
    margin: 8px;
    position: relative;
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
}
.corp {  
  height: 45rpx;
  background-color: white;
  border-bottom: 1px solid #eeeeee;
  font-size: 32rpx;
}
.info {
  background-color: white;
  display: flex;
  flex-flow: row nowrap;
}
.partl {
  height: 200rpx;
  width: 88%;
  display: flex;
  flex-flow: column nowrap;
  border: 1px solid #fff;
}
.partr {
  font-size: 30rpx;
  height: 200rpx;
  width: 12%;
  background-color: white;
  border-left: 2rpx solid #eeeeee;
  display: flex;
  flex-flow: column wrap;
  align-items: center;
  justify-content: center;
}
.nofound {
  height: 100%;
  width: 100%;
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
  justify-content: center;
}
.timeplace {
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
  justify-content: center;
}
.status {
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
  justify-content: center;
  color: green;
}

.airpname{
  width: 167rpx;
  text-align: center;
  overflow: scroll;
  text-overflow: clip;
  white-space: nowrap;
}




  
