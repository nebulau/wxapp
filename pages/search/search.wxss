/* pages/search/search.wxss */
.blank {
  height: 400rpx;
}

 .form {
  position: relative;
  height: 60px;
  display: flex;
  align-items: center;
  justify-content: center;
}
 
.dataInput {
  width: 80%;
  border: 1px solid #2c3036;
  height: 40px;
  line-height: 40px;
  font-size: 14px;
  border-radius: 20px;
  color: black;
  padding-left: 35px;
}
.card {
    border: 2px solid #ffffff;
    border-radius: 5px;
    background-color: #ffffff;
    box-shadow: 4px 1px 1px #cccccc;
    margin: 8px;
    position: relative;
    margin-top: 200rpx;
}



